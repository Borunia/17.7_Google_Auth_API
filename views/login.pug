doctype html
html
  head
    title= "Login"
    style
      include ../assets/css/login_style.css
  body
    div.container
      h1 Please Login
      div
          label Name:
          input(type="text" name="name")
      div
          label Password: 
          input(type="text" name="pass")
      
  form(action= 'http://localhost:3000/auth/google' method= 'GET')
    input(type= 'submit' value='Login')